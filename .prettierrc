{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "overrides": [
        {
            "files": [
                "*.jsonc",
            ],
            "options": {
                "parser": "json5",
                "quoteProps": "preserve",
                "tabWidth": 2,
                "singleQuote": false,
                "trailingComma": "es5",
            },
        },
    ],
}
